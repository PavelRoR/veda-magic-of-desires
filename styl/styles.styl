iframe
	border 0
section
	padding 95px 0 60px
h1
	margin 0
h3
	font-size: 44px;
	font-family: "RobotoSlabBold", sans-serif;
	color: rgb(41, 44, 56);
	line-height: 1;
	text-align: center;
	margin-bottom 75px
	span
		color #ffb43c
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.46%
		display block
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		top 0
		left 0
		right 0
		bottom 0
.button
	display block
	margin 0 auto
	border 0
	border-radius 28px
	background-image: linear-gradient( -13deg, rgb(255,160,25) 0%, rgb(255,191,80) 100%);
	box-shadow: 0px 6px 7px 0px rgba(255, 188, 74, 0.26);
	width: 262px;
	height: 56px;
	font-size: 24px;
	font-family: "RobotoSlabBold", sans-serif;
	color: rgb(255, 254, 254);
	line-height: 56px;
	text-align: center;
	&:hover
		background-image: linear-gradient( -13deg, rgb(255,191,80) 0%, rgb(255,160,25) 100%);
		color: rgb(41, 44, 56);
		text-decoration none
	&:focus
		text-decoration none
		color rgb(255, 254, 254);

