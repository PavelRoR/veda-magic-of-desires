#you_reach
	width 100%
	background-image url('../img/reach.jpg')
	background-repeat no-repeat
	background-position center
	background-size cover
	color #fff
	h3
		color #fff
		margin-bottom 85px
#reach_wrapper
	margin-bottom 55px
	overflow hidden
	.reach_container
		display table
		margin-bottom 25px
		.num_wrapper
		p
			display table-cell
			vertical-align middle
		.num_wrapper
			width 40px
			.reach_num
				width 40px
				height 40px
				border-radius: 50%;
				background-image: linear-gradient( -13deg, rgb(255,160,25) 0%, rgb(255,191,80) 100%);
				box-shadow: 0px 0px 16px 2px rgba(255, 188, 74, 0.32);
				font-size: 20px;
				font-family: "RobotoSlabBold", sans-serif;
				color: rgb(255, 255, 255);
				line-height: 40px;
				text-align: center
		p
			font-size: 20px;
			font-family: "RobotoSlabBold", sans-serif;
			color: rgb(255, 255, 255);
			line-height: 1;
			text-align: left;
			padding-left 30px